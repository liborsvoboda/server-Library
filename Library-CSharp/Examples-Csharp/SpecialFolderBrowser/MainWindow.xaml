<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:my="clr-namespace:WpfApplication1"        
        Title="MainWindow" Height="450" Width="625">
    <Grid>                
        
        <WebBrowser x:Name="webbrowser" HorizontalAlignment="Stretch"  Margin="220,20,20,20" 
                    VerticalAlignment="Stretch" />
        <ListBox x:Name="listbox" HorizontalAlignment="Left"  Margin="10,20,0,20" VerticalAlignment="Stretch" Width="200"
                 DisplayMemberPath="Name" MouseDoubleClick="listbox_MouseDoubleClick"/>

    </Grid>
</Window>
