{
  "name": "inlinelayoutswitch",
  "version": "1.0.0",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "description": "Two ideas for page transitions from an inline view to an image column or grid.",
  "scripts": {
    "start": "parcel src/index.html --open",
