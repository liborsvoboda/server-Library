  "bugs": {
    "url": "https://github.com/codrops/InlineLayoutSwitch/issues"
  },
  "devDependencies": {
    "parcel": "^2.8.0"
  },
  "dependencies": {
    "gsap": "^3.11.3",
    "imagesloaded": "^5.0.0"
  }
}
